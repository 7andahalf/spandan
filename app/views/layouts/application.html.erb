  <option value="2001">2001</option>
  <option value="2000">2000</option>
  <option value="1999">1999</option>
  </select>
  <label for="#">Student type</label>
        <select multiple class="form-control">
  <option value="mtech">MTech</option>
  <option value="imtech">IMTech</option>
  <option value="msphd">MS/PHD</option>
  </select>
  <br>
  <button type="submit" class="btn btn-default">Sign Up</button>
  <br>
   <div class="pull-left"><a href="#">Sign In</a></div>
  <div class="pull-right"><a href="#">Didn't receive confirmation instructions?</a></div>

  </form>
      </div>
