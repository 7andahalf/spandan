      <div class="container">
        <div class="row">
          <!-- Logo-->
          <div class="col-md-3 logo"> <a href="#" title="Return Home"> <img src="/img/logo.png" alt="Logo" class="logo_img"> </a> </div>
          <!-- End Logo-->

          <!-- Adds Header-->
          <div class="col-md-9 adds">
            <% if current_user.nil? %>
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#signin">Sign In
            </button>
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#signup">Sign Up
            </button>
            <% else %>

            <%= link_to('Logout', destroy_user_session_path, :method => :delete, :class => 'btn btn-primary') %>

            <% end %>

            <br>
            <br>
            <a href="#" target="_blank"> <img src="/img/adds/banner.jpg" alt="" class="img-responsive"> </a> </div>
          <!-- End Adds Header-->
        </div>
      </div>
